<div class="container">
  <h3 class="heading_inner">Sign Up Here
    <span class="deco">&nbsp;</span>
  </h3>
  <div class="clearfix margin_bottom15px"></div>
  <!--<div class="col-sm-6 col-sm-offset-3 text-center ">-->
    <div id="personal_login">

      <div class="col-sm-5 text-center ">
    <div class="col-sm-12">
      <%= form_for(resource, as: resource_name, url: registration_path(resource_name), :html=>{class:"margin_top10px", id:"sign_up"}) do |f| %>
          <%= devise_error_messages! %>
          <div class="field">
            I'm
            <label>
            <%= f.radio_button :role, 'customer'%>
           Customer</label>
            <label>
            <%= f.radio_button :role, 'vendor'%>
            Vendor</label><br />
          </div>
        <div class="input-group">
          <span id="basic-addon1" class="input-group-addon"><i class="glyphicon glyphicon-envelope"></i></span>
          <%= f.email_field :email, autofocus: true, placeholder: 'Email', class: 'form-control' %>
        </div>
        <div class="clearfix margin_bottom10px"></div>
        <div class="input-group">
          <span class="input-group-addon"><i class="glyphicon glyphicon-lock"></i> </span>
          <%= f.password_field :password, autocomplete: "off", class:"form-control", placeholder:"Password" %>
        </div>
        <div class="clearfix margin_bottom10px"></div>
        <div class="input-group">
          <span class="input-group-addon"><i class="glyphicon glyphicon-lock"></i> </span>
          <%= f.password_field :password_confirmation, autocomplete: "off", class:"form-control", placeholder:"Confirm Password" %>
        </div>
        <div class="clearfix margin_bottom10px"></div>
        <div class="input-group">
          <span class="input-group-addon"><i class="glyphicon glyphicon-phone"></i> </span>
          <%= f.text_field :mobile, autofocus: true, class:"form-control", placeholder:"Mobile"%>
        </div>
        <div class="clearfix margin_bottom10px"></div>
          <%= f.submit "Sign Up", class:"btn btn-custom pull-right"%>
      <%end%>

      <div class="clearfix margin_bottom15px"></div>

      <%- if devise_mapping.confirmable? && controller_name != 'confirmations' %>
          <%= link_to "Didn't receive confirmation instructions?", new_confirmation_path(resource_name), class:"pull-left forgot_pass" %><br />
      <% end -%>
      <div class="clearfix margin_bottom15px"></div>

      <p>Have Any Account? Please Sign in Now.</p>
      <%- if controller_name != 'sessions' %>
          <%= link_to "Sign in", new_session_path(resource_name), class:"btn btn-primary" %><br />
      <% end -%>

      <%- if devise_mapping.recoverable? && controller_name != 'passwords' && controller_name != 'registrations' %>
          <%= link_to "Forgot your password?", new_password_path(resource_name), class:"pull-left forgot_pass" %><br />
      <% end -%>


      <div class="clearfix margin_bottom10px"></div>
    </div>
  </div>
      </div>
  <div class="col-sm-2">
    <h3 class="text-center"><img src="/assets/or.png" /></h3>
  </div>

  <div class="col-sm-5">

    <div class="col-sm-12">

      <div class="row">
        <h4 class="heading">Sign Up With</h4>
      </div>
      <div class="field">
        I'm
        <label>
          <%= radio_button_tag('role', 'customer', true, class: 'role_social_login')%>
          Customer</label>
        <label>
          <%= radio_button_tag('role', 'vendor', false, class: 'role_social_login')%>
          Vendor</label><br />
      </div>
      <div id="social_login_customer">
          <div class="clearfix margin_bottom15px"></div>
          <div class="col-sm-12"><%= link_to image_tag("google-Plus_login.png"), user_omniauth_authorize_path(:google_oauth2, :role=>'customer')  %></div>
          <div class="clearfix margin_bottom15px"></div>
          <div class="col-sm-12"><%= link_to image_tag("facebook_login.png"), user_omniauth_authorize_path(:facebook, :role=>'customer') %></div>
          <div class="clearfix margin_bottom15px"></div>
          <div class="col-sm-12"><%= link_to image_tag("twitter_login.png"), user_omniauth_authorize_path(:twitter, :role=>'customer') %></div>
          <div class="clearfix margin_bottom15px"></div>
      </div>
      <div id="social_login_vendor" style="display: none;">
        <div class="clearfix margin_bottom15px"></div>
        <div class="col-sm-12"><%= link_to image_tag("google-Plus_login.png"), user_omniauth_authorize_path(:google_oauth2, :role=>'vendor')  %></div>
        <div class="clearfix margin_bottom15px"></div>
        <div class="col-sm-12"><%= link_to image_tag("facebook_login.png"), user_omniauth_authorize_path(:facebook, :role=>'vendor') %></div>
        <div class="clearfix margin_bottom15px"></div>
        <div class="col-sm-12"><%= link_to image_tag("twitter_login.png"), user_omniauth_authorize_path(:twitter, :role=>'vendor') %></div>
        <div class="clearfix margin_bottom15px"></div>
      </div>
    </div>
  </div>
